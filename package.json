{
  "name": "relata-prototype",
  "description": "Relata prototype web app",
  "private": true,
  "version": "0.1.0",
  "homepage": "https://github.com/relata/relata-prototype",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "author": {
    "name": "Christopher Setzer",
    "email": "37311998+cmsetzer@users.noreply.github.com"
  },
  "contributors": [],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "test": "npm run eslint && npm run mocha",
    "build": "cd frontend/ && npm run build && cd ../",
    "frontend-start": "cd frontend/ && npm start",
    "serve-dev": "nodemon src/ --ignore ./frontend",
    "serve-prod": "node src/",
    "dev": "run-p --print-name --race serve-dev frontend-start",
    "start": "run-s --print-name build serve-prod"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.4.3",
    "@feathersjs/authentication-local": "^4.4.3",
    "@feathersjs/authentication-oauth": "^4.4.3",
    "@feathersjs/configuration": "^4.4.3",
    "@feathersjs/errors": "^4.4.3",
    "@feathersjs/express": "^4.4.3",
    "@feathersjs/feathers": "^4.4.3",
    "citation-js": "^0.5.0-alpha.4",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^5.0.1",
    "feathers-sequelize": "^6.1.0",
    "helmet": "^3.21.2",
    "pluralize": "^8.0.0",
    "sequelize": "^5.21.3",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^4.1.1",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "eslint": "^6.5.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "npm-check": "^5.9.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.18.2"
  }
}
