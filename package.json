{
  "name": "relata-prototype",
  "description": "Prototype web app for the Relata Project",
  "private": true,
  "version": "0.1.0",
  "homepage": "https://github.com/relata/relata-prototype",
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^10.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "eslint": "eslint src/. test/. --config .eslintrc.json",
    "mocha": "mocha test/ --recursive --exit",
    "test": "npm run eslint && npm run mocha",
    "build": "cd frontend/ && npm run build && cd ../",
    "frontend-start": "cd frontend/ && npm start",
    "serve-dev": "nodemon src/ --ignore ./frontend",
    "serve-prod": "node src/",
    "dev": "NODE_ENV=development run-p --print-name --race serve-dev frontend-start",
    "start": "run-s --print-name build serve-prod"
  },
  "dependencies": {
    "@feathersjs/authentication": "^4.5.11",
    "@feathersjs/authentication-local": "^4.5.11",
    "@feathersjs/authentication-oauth": "^4.5.11",
    "@feathersjs/configuration": "^4.5.11",
    "@feathersjs/errors": "^4.5.11",
    "@feathersjs/express": "^4.5.11",
    "@feathersjs/feathers": "^4.5.11",
    "@fortawesome/fontawesome-free": "^5.15.2",
    "academicons": "^1.8.6",
    "citation-js": "^0.5.0-alpha.10",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "date-fns": "^2.17.0",
    "email-validator": "^2.0.4",
    "express-session": "^1.17.1",
    "feathers-authentication-hooks": "^1.0.2",
    "feathers-hooks-common": "^5.0.5",
    "feathers-sequelize": "^6.2.0",
    "flexsearch": "^0.6.32",
    "helmet": "^4.4.1",
    "indefinite": "^2.4.1",
    "memorystore": "^1.6.4",
    "pluralize": "^8.0.0",
    "sequelize": "^6.5.0",
    "serve-favicon": "^2.5.0",
    "sqlite3": "^5.0.3",
    "string-similarity": "^4.0.4",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "axios": "^0.21.1",
    "mocha": "^8.3.0",
    "node-fetch": "^2.6.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1"
  }
}
